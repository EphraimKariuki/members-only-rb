<%= form_with model: [@user, @post]  do |form|%>
    <div class="form-group">
        <%= form.label :title %>
        <%= form.text_field :title, class: 'form-control' %>
    </div>

    <div class="form-group">
        <%= form.label :body %>
        <%= form.text_area :body, class: 'form-control' %>
    </div>

    <div class="form-group">
        <%= form.submit 'Create Post', class: 'btn btn-primary' %>
    </div>
<% end%>